[{
  "name": "Yelp",
  "plugin": "yelp",
  "canonicalURL": "https://www.yelp.com/",
  "regex": "http(s)*://(www.)*yelp.com",
  "supported": ["Reviews"],
  "unsupported": ["Comments", "Photographs", "Messages", "Events"]
}, {
  "name": "IMDB",
  "plugin": "imdb",
  "deps": ["papaparse"],
  "canonicalURL": "http://www.imdb.com/",
  "regex": "http(s)*://(www.)*imdb.com",
  "supported": ["Reviews", "Ratings"],
  "unsupported": ["Comments", "Lists"]
},
{
  "name": "Goodreads",
  "plugin": "goodreads",
  "deps": ["papaparse"],
  "canonicalURL": "https://www.goodreads.com/",
  "regex": "http(s)*://(www.)*goodreads.com",
  "supported": ["Reviews", "Ratings"],
  "unsupported": ["Comments", "Lists", "Quotes"]
},
{
  "name": "TripAdvisor",
  "plugin": "tripadvisor",
  "canonicalURL": "https://www.tripadvisor.com/",
  "regex": "http(s)*://(www.)*tripadvisor.com",
  "supported": ["Reviews"],
  "unsupported": ["Maps", "Comments", "Questions", "Photographs", "Lists"]
},
{
  "name": "Amazon.com",
  "plugin": "amazon",
  "canonicalURL": "https://www.amazon.com/",
  "regex": "http(s)*://(www.)*amazon.com",
  "supported": ["Reviews"],
  "unsupported": ["Ratings", "Comments", "Multimedia", "Lists"]
},
{
  "name": "Quora",
  "plugin": "quora",
  "canonicalURL": "https://www.quora.com/",
  "regex": "http(s)*://(www.)*quora.com",
  "supported": ["Answers"],
  "unsupported": ["Questions","Posts"],
  "extraPermissions": true
}
]
